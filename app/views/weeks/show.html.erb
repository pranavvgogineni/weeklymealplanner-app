<h1><%= @week.title%></h1>
<% @recipes.each do |recipe| %>
  <p><%= recipe.title%></p>
  <form method="post" action="/weeklymeals">
    <input name="recipe_id" value="<%=recipe.id%>" type="hidden"/>
    <input name="week_id" value="<%=@week.id%>" type="hidden"/>
    <input type="submit" value="Add to week"/>
  </form>
<% end %>